class BankAccount {
    String name;
    double balance;

    BankAccount() {
        name = "Aaryan";
        balance = 25000;
    }

    void display() {
        System.out.println("Account Holder: " + name);
        System.out.println("Balance: " + balance);
    }

    public static void main(String args[]) {
        BankAccount b = new BankAccount();
        b.display();
    }
}
